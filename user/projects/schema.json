{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Projects",
	"description": "List of projects",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"name": {
				"type": "string"
			},
			"description": {
				"type": "string"
			},
			"url": {
				"type": "string",
				"format": "uri"
			},
			"icon": {
				"type": "string",
				"format": "uri"
			}
		},
		"required": ["name", "description", "url", "icon"],
		"additionalProperties": false
	}
}
