{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Experiences",
	"description": "List of experiences",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"title": {
				"type": "string"
			},
			"at": {
				"type": "string"
			},
			"time": {
				"type": "string",
				"format": "date"
			}
		},
		"required": ["title", "at"],
		"additionalProperties": false
	}
}
