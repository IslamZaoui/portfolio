{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Uses",
	"description": "List of My Tools",
	"type": "object",
	"additionalProperties": {
		"type": "array",
		"items": {
			"type": "object",
			"properties": {
				"name": {
					"type": "string"
				},
				"description": {
					"type": "string"
				},
				"url": {
					"type": "string",
					"format": "uri"
				}
			},
			"required": ["name"],
			"additionalProperties": false
		}
	}
}
